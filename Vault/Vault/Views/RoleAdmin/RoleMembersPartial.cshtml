@model IEnumerable<Vault.Models.AppUserModel>
    @if (Model.Count() != 0)
    {
        foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td><input type="checkbox" name="UsersToDelete" value="@user.Id" /></td>
            </tr>
        }
    }
    else
    {
        <tr><td colspan="2" class="text-center">No users in group</td></tr>
    }