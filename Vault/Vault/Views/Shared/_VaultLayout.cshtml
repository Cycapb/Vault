<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>@ViewBag.Title</title>
    @Styles.Render("~/bundles/datetimepicker/css", "~/bundles/sitestyles/css")
    @Scripts.Render("~/bundles/datetimepicker", "~/bundles/validation")
</head>
<body>
<div class="container">
    @RenderSection("calendarScripts", false)
    @if (TempData["message"] != null)
    {
        <div class="alert alert-success">@TempData["message"]</div>
    }
    @RenderBody()
</div>
</body>
</html>
