@model IEnumerable<VaultDAL.Models.VaultUser>
    @if (Model != null && Model.Count() != 0)
    {
      foreach (var user in Model)
       {
           <tr>
               <td><input type="checkbox" value="@user.Id"/></td>
               <td>@user.Id</td>
               <td>@user.UserName</td>
           </tr>     
       }
    }
    else
    {
        <tr>
            <td colspan="3" class="text-center">No users</td>
        </tr>
    }